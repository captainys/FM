ALL : EXE\MAKED77.EXP

# BUILD:
#	MD BUILD

# EXE:
#	MD EXE


BUILD\RS232C32.OBJ : ..\XMODEM\RS232C32.ASM
	386asm ..\XMODEM\RS232C32.ASM -OBJECT BUILD\RS232C32.OBJ



BUILD\ASM.OBJ : ASM.ASM
	386asm ASM.ASM -OBJECT BUILD\ASM.OBJ



BUILD\MAKED77.OBJ : MAKED77.C
	HCD386 MAKED77.C -OBJ BUILD\MAKED77.OBJ

BUILD\XMEMSEND.OBJ : XMEMSEND.C
	HCD386 XMEMSEND.C -OBJ BUILD\XMEMSEND.OBJ


EXE\MAKED77.EXP : BUILD\MAKED77.OBJ BUILD\XMEMSEND.OBJ BUILD\RS232C32.OBJ BUILD\ASM.OBJ
	386LINK BUILD\MAKED77.OBJ BUILD\ASM.OBJ BUILD\RS232C32.OBJ BUILD\XMEMSEND.OBJ @LIBS.LN -EXE EXE\MAKED77.EXP -SYMFILE MAKED77.SYM
